      [
        {
          "files": ["**/**.md", "**/**.yml"],
          "exclude": ["**/obj/**"]
        }
      ],
    "resource": [
        {
          "files": ["**/images/**"],
          "exclude": ["**/obj/**"]
        }
    ],
    "globalMetadata": {
        "ROBOTS": "INDEX, FOLLOW",
