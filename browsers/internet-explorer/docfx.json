          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/includes/**",
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
